<TAN-key xmlns="tag:textalign.net,2015:ns" TAN-version="1/dev"
    id="tag:textalign.net,2015:tan-key:key-types">
    <head>
        <name>TAN keywords for types of keys</name>
        <desc>Definitive list of key terms used for types of keys.</desc>
        <master-location href="http://textalign.net/release/1/dev/TAN-key/key-types.TAN-key.xml"/>
        <rights-excluding-sources rights-holder="kalvesmaki">
            <IRI>http://creativecommons.org/licenses/by/4.0/deed.en_US</IRI>
            <name>Creative Commons Attribution 4.0 International License</name>
            <desc>This license is granted independent of rights and licenses associated with the
                source. </desc>
        </rights-excluding-sources>
        <declarations/>
        <agent xml:id="kalvesmaki" roles="creator">
            <IRI>http://viaf.org/viaf/299582703</IRI>
            <IRI>tag:textalign.net,2015:agent:kalvesmaki:joel</IRI>
            <name xml:lang="eng">Joel Kalvesmaki</name>
        </agent>
        <role xml:id="creator">
            <IRI>http://schema.org/creator</IRI>
            <name xml:lang="eng">creator</name>
        </role>
        <change when="2016-04-23" who="kalvesmaki">Started file</change>
    </head>
    <body in-progress="false" affects-element="key-type">
        <item>
            <IRI>tag:textalign.net,2015:key-type:base</IRI>
            <name>base</name>
            <desc>@key is the result of applying tan:string-base($i), where $i is
                the value of the token chosen.
            </desc>
        </item>
        <item>
            <IRI>tag:textalign.net,2015:key-type:root</IRI>
            <name>root</name>
            <desc>@key is the root of the token chosen</desc>
        </item>
        <item>
            <IRI>tag:textalign.net,2015:key-type:start1</IRI>
            <name>^.</name>
            <name>start1</name>
            <name>a</name>
            <desc>@key is the first letter of the token chosen</desc>
        </item>
        <item>
            <IRI>tag:textalign.net,2015:key-type:start2</IRI>
            <name>^..</name>
            <name>start2</name>
            <name>ab</name>
            <desc>@key is the first two letters of the token chosen</desc>
        </item>
        <item>
            <IRI>tag:textalign.net,2015:key-type:start3</IRI>
            <name>^...</name>
            <name>start3</name>
            <name>abc</name>
            <desc>@key is the first three letters of the token chosen</desc>
        </item>
        <item>
            <IRI>tag:textalign.net,2015:key-type:end1</IRI>
            <name>.$</name>
            <name>end1</name>
            <name>z</name>
            <desc>@key is the last letter of the token chosen</desc>
        </item>
        <item>
            <IRI>tag:textalign.net,2015:key-type:end2</IRI>
            <name>..$</name>
            <name>end2</name>
            <name>yz</name>
            <desc>@key is the last two letters of the token chosen</desc>
        </item>
        <item>
            <IRI>tag:textalign.net,2015:key-type:end3</IRI>
            <name>...$</name>
            <name>end3</name>
            <name>xyz</name>
            <desc>@key is the last three letters of the token chosen</desc>
        </item>
    </body>
</TAN-key>
