<TAN-R-mor TAN-version="1" id="tag:kalvesmaki.com,2014:tan-r-mor:grc:perseus"
   xmlns="tag:textalign.net,2015:ns">
   <head>
      <name>Perseus rules for Greek morphology</name>
      <desc xml:lang="eng">Model of a grammar code to be used in a TAN declaration concerning
         grammar</desc>
      <master-location>http://kalvesmaki.com/tan-library/tan-r-mor/grc.kalvesmaki.com,2014.1.xml</master-location>
      <rights-excluding-sources rights-holder="kalvesmaki">
         <IRI>http://creativecommons.org/licenses/by/4.0/deed.en_US</IRI>
         <name>Creative Commons Attribution 4.0 International License</name>
         <name>This license is granted independent of rights and licenses associated with the
            source. </name>
      </rights-excluding-sources>
      <source xml:id="perseus">
         <IRI>urn:uuid:147d8040-4ca9-11e3-848f-0002a5d5c51b:2013-11-18</IRI>
         <name>Perseus codes</name>
         <desc xml:lang="eng">Perseus's standard code for Greek morphology (a string of 10 chars.),
            found at http://perseus.uchicago.edu/about.html (accessed 2013-11-18)</desc>
      </source>
      <source xml:id="olia">
         <IRI>urn:uuid:835d029e-b745-47b6-b657-cccef18a64da</IRI>
         <name>Ontologies of Linguistic Annotation (OLiA)</name>
      </source>
      <declarations ed-when="2015-03-03" ed-who="kalvesmaki">
         <for-lang>grc</for-lang>
         <feature xml:id="adjective">
            <IRI>http://purl.org/olia/olia.owl#Adjective</IRI>
            <name>adjective</name>
         </feature>
         <feature xml:id="conjunction">
            <IRI>http://purl.org/olia/olia.owl#Conjunction</IRI>
            <name>conjunction</name>
         </feature>
         <feature xml:id="adverb">
            <IRI>http://purl.org/olia/olia.owl#Adverb</IRI>
            <name>adverb</name>
         </feature>
         <feature xml:id="ω">
            <IRI>tag:textalign.net,2015:morphology:option:ωvocative</IRI>
            <name>ω vocative</name>
         </feature>
         <feature xml:id="particle">
            <IRI>http://purl.org/olia/olia.owl#Unique</IRI>
            <name>particle (unique)</name>
            <desc>The category "Unique" approximates the linguistic concept "Particle". It covers
               categories with unique or very small membership, such as negative particle, which are
               `unassigned' to any of the standard part-of-speech categories.
               (http://www.ilc.cnr.it/EAGLES96/annotate/node16.html#mp 19.09.06) </desc>
         </feature>
         <feature xml:id="interjection">
            <IRI>http://purl.org/olia/olia.owl#Interjection</IRI>
            <name>interjection</name>
         </feature>
         <feature xml:id="cardinal" ed-when="2015-03-10" ed-who="kalvesmaki">
            <IRI>http://purl.org/olia/olia.owl#CardinalNumber</IRI>
            <name>cardinal number</name>
         </feature>
         <feature xml:id="noun">
            <IRI>http://purl.org/olia/olia.owl#Noun</IRI>
            <name>noun</name>
         </feature>
         <feature xml:id="pronoun">
            <IRI>http://purl.org/olia/olia.owl#Pronoun</IRI>
            <name>pronoun</name>
         </feature>
         <feature xml:id="preposition">
            <IRI>http://purl.org/olia/olia.owl#Preposition</IRI>
            <name>preposition</name>
         </feature>
         <feature xml:id="verb">
            <IRI>http://purl.org/olia/olia.owl#Verb</IRI>
            <name>verb</name>
         </feature>
         <feature xml:id="acronym">
            <IRI>http://purl.org/olia/olia.owl#Acronym</IRI>
            <name>acronym</name>
         </feature>
         <feature xml:id="article">
            <IRI>http://purl.org/olia/olia.owl#Article</IRI>
            <name>article or determinative</name>
         </feature>
         <feature xml:id="reciprocal">
            <IRI>http://purl.org/olia/olia.owl#ReciprocalPronoun</IRI>
            <name>reciprocal pronoun</name>
         </feature>
         <feature xml:id="demonstrative">
            <IRI>http://purl.org/olia/olia.owl#PronounDemonstrative</IRI>
            <name>demonstrative pronoun</name>
         </feature>
         <feature xml:id="proper">
            <IRI>http://purl.org/olia/olia.owl#NounProper</IRI>
            <name>proper</name>
         </feature>
         <feature xml:id="interrogative">
            <IRI>tag:textalign.net,2015:morphology:grc:perseus:interrogative</IRI>
            <name>interrogative</name>
            <desc>To be applied only to determiners and pronouns, quantifiers
               http://purl.org/olia/olia.owl#InterrogativeDeterminer
               http://purl.org/olia/olia.owl#InterrogativePronoun </desc>
         </feature>
         <feature xml:id="reflexive">
            <IRI>http://purl.org/olia/olia.owl#ReflexivePronoun</IRI>
            <name>reflexive</name>
         </feature>
         <feature xml:id="modal">
            <IRI>http://purl.org/olia/olia.owl#ModalParticle</IRI>
            <name>modal particle</name>
         </feature>
         <feature xml:id="personal">
            <IRI>http://purl.org/olia/olia.owl#PersonalPronoun</IRI>
            <name>personal pronoun</name>
         </feature>
         <feature xml:id="relative">
            <IRI>http://purl.org/olia/olia.owl#RelativePronoun</IRI>
            <name>relative pronoun</name>
         </feature>
         <feature xml:id="possessive">
            <IRI>http://purl.org/olia/olia.owl#PossessivePronoun</IRI>
            <name>possessive pronoun</name>
         </feature>
         <feature xml:id="verbal">
            <IRI>tag:textalign.net,2015:morphology:grc:perseus:adjective-verbal</IRI>
            <name>verbal</name>
            <comment when="2015-05-05" who="kalvesmaki">Deprecated option</comment>
         </feature>
         <feature xml:id="indefinite">
            <IRI>http://purl.org/olia/olia.owl#IndefinitePronoun</IRI>
            <name>indefinite</name>
         </feature>
         <feature xml:id="first">
            <IRI>http://purl.org/olia/olia.owl#First</IRI>
            <name>first person</name>
         </feature>
         <feature xml:id="second">
            <IRI>http://purl.org/olia/olia.owl#Second</IRI>
            <name>second person</name>
         </feature>
         <feature xml:id="third">
            <IRI>http://purl.org/olia/olia.owl#Third</IRI>
            <name>third person</name>
         </feature>
         <feature xml:id="dual">
            <IRI>http://purl.org/olia/olia.owl#Dual</IRI>
            <name>dual</name>
         </feature>
         <feature xml:id="plural">
            <IRI>http://purl.org/olia/olia.owl#Plural</IRI>
            <name>plural</name>
         </feature>
         <feature xml:id="singular">
            <IRI>http://purl.org/olia/olia.owl#Singular</IRI>
            <name>singular</name>
         </feature>
         <feature xml:id="aorist">
            <IRI>http://purl.org/olia/olia.owl#Aorist</IRI>
            <name>aorist</name>
         </feature>
         <feature xml:id="future">
            <IRI>http://purl.org/olia/olia.owl#Future</IRI>
            <name>future</name>
         </feature>
         <feature xml:id="imperfect">
            <IRI>http://purl.org/olia/olia.owl#Imperfect</IRI>
            <name>imperfect</name>
         </feature>
         <feature xml:id="pluperfect">
            <IRI>http://purl.org/olia/olia.owl#PluperfectTense</IRI>
            <name>pluperfect</name>
         </feature>
         <feature xml:id="present">
            <IRI>http://purl.org/olia/olia.owl#Present</IRI>
            <name>present</name>
         </feature>
         <feature xml:id="perfect">
            <IRI>http://purl.org/olia/olia.owl#Perfect</IRI>
            <name>perfect</name>
         </feature>
         <feature xml:id="future-perfect">
            <IRI>http://purl.org/olia/olia.owl#FuturePerfect</IRI>
            <name>future perfect</name>
         </feature>
         <feature xml:id="indicative">
            <IRI>http://purl.org/olia/olia.owl#IndicativeMood</IRI>
            <name>indicative</name>
            <comment who="kalvesmaki" when="2015-02-28">What is the difference between
               http://purl.org/olia/olia.owl#IndicativeMood and
               http://purl.org/olia/olia.owl#IndicativeVerb ? </comment>
         </feature>
         <feature xml:id="imperative">
            <IRI>http://purl.org/olia/olia.owl#ImperativeVerb</IRI>
            <name>imperative</name>
         </feature>
         <comment who="kalvesmaki" when="2015-02-28">What is the difference between
            http://purl.org/olia/olia.owl#ImperativeVerb and
            http://purl.org/olia/olia.owl#ImperativeModality ? </comment>
         <feature xml:id="infinitive">
            <IRI>http://purl.org/olia/olia.owl#Infinitive</IRI>
            <name>infinitive</name>
         </feature>
         <feature xml:id="optative">
            <IRI>http://purl.org/olia/olia.owl#OptativeMood</IRI>
            <name>optative</name>
            <comment who="kalvesmaki" when="2015-02-28">What is the difference between
               http://purl.org/olia/olia.owl#OptativeMood and
               http://purl.org/olia/olia.owl#OptativeModality ? </comment>
         </feature>
         <feature xml:id="participle">
            <IRI>http://purl.org/olia/olia.owl#Participle</IRI>
            <name>participle</name>
         </feature>
         <feature xml:id="subjunctive">
            <IRI>http://purl.org/olia/olia.owl#SubjunctiveModality</IRI>
            <name>subjunctive</name>
            <comment who="kalvesmaki" when="2015-02-28">What is the difference between
               http://purl.org/olia/olia.owl#SubjunctiveMood and
               http://purl.org/olia/olia.owl#SubjunctiveModality ? </comment>
         </feature>
         <feature xml:id="active">
            <IRI>http://purl.org/olia/olia.owl#ActiveVoice</IRI>
            <name>active</name>
         </feature>
         <feature xml:id="middle-passive">
            <IRI>tag:textalign.net,2015:morphology:grc:perseus:voice-middlepassive</IRI>
            <name>middle-passive</name>
            <desc>Verbal form that ambiguously middle and passive, the union of
               http://purl.org/olia/olia.owl#MiddleVoice and
               http://purl.org/olia/olia.owl#PassiveVoice </desc>
         </feature>
         <feature xml:id="middle">
            <IRI>http://purl.org/olia/olia.owl#MiddleVoice</IRI>
            <name>middle</name>
         </feature>
         <feature xml:id="passive">
            <IRI>http://purl.org/olia/olia.owl#PassiveVoice</IRI>
            <name>passive</name>
         </feature>
         <feature xml:id="common">
            <IRI>http://purl.org/olia/olia.owl#CommonGender</IRI>
            <name>common</name>
         </feature>
         <feature xml:id="feminine">
            <IRI>http://purl.org/olia/olia.owl#Feminine</IRI>
            <name>feminine</name>
         </feature>
         <feature xml:id="masculine">
            <IRI>http://purl.org/olia/olia.owl#Masculine</IRI>
            <name>masculine</name>
         </feature>
         <feature xml:id="neuter">
            <IRI>http://purl.org/olia/olia.owl#Neuter</IRI>
            <name>neuter</name>
         </feature>
         <feature xml:id="accusative">
            <IRI>http://purl.org/olia/olia.owl#Accusative</IRI>
            <name>accusative</name>
         </feature>
         <feature xml:id="dative">
            <IRI>http://purl.org/olia/olia.owl#DativeCase</IRI>
            <name>dative</name>
         </feature>
         <feature xml:id="genitive">
            <IRI>http://purl.org/olia/olia.owl#GenitiveCase</IRI>
            <name>genitive</name>
         </feature>
         <feature xml:id="nominative">
            <IRI>http://purl.org/olia/olia.owl#Nominative</IRI>
            <name>nominative</name>
         </feature>
         <feature xml:id="vocative">
            <IRI>http://purl.org/olia/olia.owl#VocativeCase</IRI>
            <name>vocative</name>
         </feature>
         <feature xml:id="comparative">
            <IRI>http://purl.org/olia/olia.owl#Comparative</IRI>
            <name>comparative</name>
         </feature>
         <feature xml:id="superlative">
            <IRI>http://purl.org/olia/olia.owl#Superlative</IRI>
            <name>superlative</name>
         </feature>
      </declarations>
      <agent xml:id="kalvesmaki" roles="editor">
         <IRI>http://viaf.org/viaf/299582703</IRI>
         <IRI>tag:kalvesmaki.com,2014:self</IRI>
         <name>Joel Kalvesmaki</name>
      </agent>
      <role xml:id="editor">
         <IRI>http://schema.org/editor</IRI>
         <name>Editor</name>
      </role>
      <change who="kalvesmaki" when="2015-05-04">Converted IRI scheme to OLiA.</change>
      <change who="kalvesmaki" when="2014-02-26">Drastically revised in light of creating</change>
      <change when="2014-02-05" who="kalvesmaki">Revised header in light of earlier core TAN RNG
         changes.</change>
      <change when="2013-11-23" who="kalvesmaki">Changed data element name to IRI, anticipating
         multiple IRIs.</change>
      <change when="2013-11-18" who="kalvesmaki">Adjusted uuid-date name. Built new roles
         elements.</change>
      <change when="2013-11-16" who="kalvesmaki">First draft started.</change>
      <comment when="2013-11-16" who="kalvesmaki">This was written to remind users of the TAN system
         in ancient Greek what the various fields mean in the 10-character codes developed by
         Perseus. </comment>
   </head>
   <body>
      <!-- 1st category -->
      <report code-regex-test="^c[^ -]+">A conjunction has no other inflectional properties.</report>
      <report cert="high" code-regex-test="^e">Code e has been deprecated. It was used in some cases for ὦ
         voc., to be distinguished from ὦ as exclamation </report>
      <report code-regex-test="^g [^-m]+">A particle is either qualified as modal (g m) or has no other
         inflectional properties.</report>
      <report code-regex-test="^i[^ -]+">An interjection has no other inflectional properties.</report>
      <report code-regex-test="^r[^ -]+">A preposition has no other inflectional properties.</report>
      <report code-regex-test="^y[^ -]+">An acronym has no other inflectional properties.</report>
      <!-- 2nd category -->
      <report code-regex-test="^[^dp] i">The category interrogative may be applied only to determiners and
         pronouns.</report>
      <report cert="high" code-regex-test="^. v">Code v for verbal adjective has been deprecated. </report>
      <category>
         <IRI>http://purl.org/olia/olia.owl#MorphosyntacticCategory</IRI>
         <comment who="kalvesmaki" when="2015-02-28">Is there any value in providing the dbpedia URL
            as a synonym? Or is that just plain wrong? Or treacherous?</comment>
         <name>Morphosyntactic Category</name>
         <option feature="adjective" code="a"/>
         <option feature="conjunction" code="c"/>
         <option feature="adverb" code="d"/>
         <option feature="ω" code="e"/>
         <option feature="particle" code="g"/>
         <option feature="interjection" code="i"/>
         <option feature="cardinal" code="m" ed-when="2015-03-10" ed-who="kalvesmaki"/>
         <option feature="noun" code="n"/>
         <option feature="pronoun" code="p"/>
         <option feature="preposition" code="r"/>
         <option feature="verb" code="v"/>
         <option feature="acronym" code="y"/>
      </category>
      <category>
         <IRI>tag:textalign.net,2015:morphology:category:morphosyntactic-secondary</IRI>
         <name>secondary morphosyntactic category</name>
         <comment who="kalvesmaki" when="2015-05-05">I am uncertain if this approach to naming is
            sound</comment>
         <option feature="article" code="a"/>
         <option feature="reciprocal" code="c"/>
         <option feature="demonstrative" code="d"/>
         <option feature="proper" code="e"/>
         <option feature="interrogative" code="i"/>
         <option feature="reflexive" code="k"/>
         <option feature="modal" code="m"/>
         <option feature="personal" code="p"/>
         <option feature="relative" code="r"/>
         <option feature="possessive" code="s"/>
         <option feature="verbal" code="v"/>
         <option feature="indefinite" code="x"/>
      </category>
      <category>
         <IRI>http://purl.org/olia/olia.owl#PersonFeature</IRI>
         <IRI>http://dbpedia.org/resource/Grammatical_person</IRI>
         <name>person</name>
         <option feature="first" code="1"/>
         <option feature="second" code="2"/>
         <option feature="third" code="3"/>
      </category>
      <category>
         <IRI>http://purl.org/olia/olia.owl#NumberFeature</IRI>
         <IRI>http://dbpedia.org/resource/Grammatical_number</IRI>
         <name>number</name>
         <option feature="dual" code="d"/>
         <option feature="plural" code="p"/>
         <option feature="singular" code="s"/>
      </category>
      <category>
         <IRI>http://purl.org/olia/olia.owl#TenseFeature</IRI>
         <name>tense</name>
         <option feature="aorist" code="a"/>
         <option feature="future" code="f"/>
         <option feature="imperfect" code="i"/>
         <option feature="pluperfect" code="l"/>
         <option feature="present" code="p"/>
         <option feature="perfect" code="r"/>
         <option feature="future-perfect" code="t"/>
      </category>
      <category>
         <IRI>http://purl.org/olia/olia-top.owl#MoodFeature</IRI>
         <name>mood</name>
         <option feature="indicative" code="i"/>
         <option feature="imperative" code="m"/>
         <option feature="infinitive" code="n"/>
         <option feature="optative" code="o"/>
         <option feature="participle" code="p"/>
         <option feature="subjunctive" code="s"/>
      </category>
      <category>
         <IRI>http://dbpedia.org/resource/VoiceFeature</IRI>
         <name>voice</name>
         <option feature="active" code="a"/>
         <option feature="middle-passive" code="e"/>
         <option feature="middle" code="m"/>
         <option feature="passive" code="p"/>
      </category>
      <category>
         <IRI>http://dbpedia.org/resource/GenderFeature</IRI>
         <name>gender</name>
         <option feature="common" code="c"/>
         <option feature="feminine" code="f"/>
         <option feature="masculine" code="m"/>
         <option feature="neuter" code="n"/>
      </category>
      <category>
         <IRI>http://dbpedia.org/resource/CaseFeature</IRI>
         <name>case</name>
         <option feature="accusative" code="a"/>
         <option feature="dative" code="d"/>
         <option feature="genitive" code="g"/>
         <option feature="nominative" code="n"/>
         <option feature="vocative" code="v"/>
      </category>
      <category>
         <IRI>http://dbpedia.org/resource/DegreeFeature</IRI>
         <name>degree</name>
         <option feature="comparative" code="c"/>
         <option feature="superlative" code="s"/>
      </category>
   </body>
</TAN-R-mor>
